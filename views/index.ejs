<!-- Header -->
<header class="w3-display-container w3-content w3-wide" style="max-width:1500px;" id="home">
  <img class="w3-image" src="/images/forestbridge.jpg" alt="Architecture" width="1500" height="800">
  <div class="w3-display-middle w3-margin-top w3-center">
    <h1 class="w3-xxlarge w3-text-white"><span class="w3-padding w3-black w3-opacity-min"><b>See</b></span> <span class="w3-hide-small w3-text-light-grey">And Chat</span></h1>
  </div>
</header>
<!-- Page content -->
<div class="w3-content " style="max-width:1200px">
  <!-- About Section -->
  <div class="w3-row w3-padding-64" id="about">
    <div class="w3-col w3-padding-large w3-padding-32 w3-margin-top">
